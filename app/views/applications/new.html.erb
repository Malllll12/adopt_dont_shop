<%= form_with url: "/applications", method: :post, local: true do |f| %>
  <%= f.label :applicant_name %>
  <%= f.text_field :applicant_name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.label :reason %>
  <%= f.text_field :reason %>

  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.submit "Submit" %>
<% end %>
